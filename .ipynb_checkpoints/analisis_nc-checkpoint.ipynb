    "    hmax.values[hmax==0]=['nan']\n",
    "    hmax.rename('eta_'+str(j)).plot.imshow(aspect=ds.dims['lon'] / ds.dims['lat'],size=7,cmap=\"coolwarm\",norm=divnorm)\n",
